package edu.neu.csye6200;

public class Person extends AbstractPerson{
	private int id;
	private int age;
	private String name;
	private double weightLbs;
	private double weightLossLbs;
	Person(){
		
	}
	public Person(int id, int age, String name, double weightLbs, double weightLossLbs) {
		super();
		this.id = id;
		this.age = age;
		this.name = name;
		this.weightLbs = weightLbs;
		this.weightLossLbs = weightLossLbs;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getWeightLbs() {
		return weightLbs;
	}
	public void setWeightLbs(double weightLbs) {
		this.weightLbs = weightLbs;
	}
	public double getWeightLossLbs() {
		return weightLossLbs;
	}
	public void setWeightLossLbs(double weightLossLbs) {
		this.weightLossLbs = weightLossLbs;
	}
	@Override
	public String toString() {
		return "Person [id=" + id + ", age=" + age + ", name=" + name + ", weightLbs=" + weightLbs + ", weightLossLbs="
				+ weightLossLbs + "]"+"\n";
	}
	
	
	
	
	
	
}
