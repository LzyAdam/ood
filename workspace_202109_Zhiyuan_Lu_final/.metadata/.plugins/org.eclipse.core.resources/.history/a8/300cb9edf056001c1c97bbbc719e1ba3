package edu.neu.csye6200;

import java.util.List;

public abstract class AbstractClub {
public abstract void addPerson(AbstractPerson abstractPerson);
	
	public abstract String getPersonInfo();	
	public abstract void sortandshowPerson();	

    public abstract List getWeightLossStats();

	public abstract void sortPersonByName();

	public abstract void sortPersonByWeightLoss();
	
	public abstract void graphCurrentWeightLoss();
	
	public abstract void graphProjectedWeightLossByOneMonth();
	
	public abstract void graphProjectedWeightLossByOneYear();
}
