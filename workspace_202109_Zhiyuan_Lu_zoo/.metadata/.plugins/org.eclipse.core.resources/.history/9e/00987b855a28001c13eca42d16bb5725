package school;

public class ValueTransferTest {
   public static void main(String[] args) {
	int m=10;
	int n=m;
	System.out.println("m= "+m+"n= "+n);
	n=20;
	System.out.println("m= "+m+"n= "+n);
	
	
	
	Order o1=new Order();
	o1.orderid=1001;
	System.out.println("o1="+o1);
	Order o2=o1;
	System.out.println("o1=  "+o1.orderid+", o2= "+ o2.orderid );
	
	
	
}
}
class Order{
	int orderid;
}