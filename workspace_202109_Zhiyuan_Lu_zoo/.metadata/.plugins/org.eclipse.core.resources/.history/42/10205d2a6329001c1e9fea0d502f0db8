class Person{
	private String name;
    private int age;
    
    
    public Person() {
    	this.eat();
    }   
    
    public Person(String name) {
    	this();
    	this.name=name;
    }
    
    public Person(int age) {
    	this("name");
    	this.age=age;
    	System.out.println(this.age );
    }
   
   public void eat() {
	   System.out.println("eat fast food");
   } 
    public void setname(String name) {
    	this.name=name;
    }
     /*
