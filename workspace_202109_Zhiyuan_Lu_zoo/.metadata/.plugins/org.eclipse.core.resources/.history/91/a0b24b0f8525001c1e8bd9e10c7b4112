package Classwork;

public class Student {
	String name;
	int age;
	String major;
	public Student() {}
	public Student(String name,int age,String major)
	{this.name=name;this.age=age;this.major=major;}
	public void eat() {
		System.out.println("eat");
	}
	public void sleep() {
		System.out.println("Sleep");
	}
	public void study() {
		System.out.println("study");
	}
	
	
	

}
