package school;

public class PersonTest {
	public static void main(String[] args) {
		Person p1=new Person();
		p1.setage(23);
		System.out.println(p1.getage());
	}

}

class Person{
	private String name;
    private int age;
    
    public void setname(String n) {
    	name=n;
    }
    
    public String getname() {
    	return name;
    }
    public void setage(int a) {
    	age=a;
    }
    public int getage() {
    	return age;
    }
    
}